{
  "name": "ihop-reach",
  "description": "An application for users to access biological data extracted from biomedical literature.",
  "version": "0.1.0",
  "license": "LGPL-3.0",
  "scripts": {
    "build": "node --max_old_space_size=10240 ./node_modules/.bin/gatsby build",
    "devbuild": "GATSBY_GRAPHQL_API_HOST=http://localhost:8080 npm run build",
    "develop": "GATSBY_GRAPHQL_API_HOST=http://localhost:8080 gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve -p 8080",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootswatch": "^4.3.1",
    "flow-bin": "^0.100.0",
    "font-awesome": "^4.7.0",
    "gatsby": "^2.7.3",
    "gatsby-plugin-flow": "^1.0.6",
    "gatsby-plugin-google-analytics": "^2.0.20",
    "gatsby-plugin-lunr": "^1.5.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-source-graphql": "^2.0.18",
    "gatsby-source-mongodb": "^2.0.19",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-cookie": "^4.0.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "slug": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-gatsby": "^0.1.11",
    "eslint": "^5.16.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "flow-bin": "^0.100.0",
    "prettier": "^1.17.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cannin/ihop-reach"
  },
  "bugs": {
    "url": "https://github.com/cannin/ihop-reach/issues"
  }
}
